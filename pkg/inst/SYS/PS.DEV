 define [char_width(x)] [%char_width=x]
 define [color(X,Y,Z,U)] X[S]Y[S]Z[S]U[S]setcmykcolor[LF]
 define [CYAN] [color(1,0,0,0)]
 define [MAGENTA] [color(0,1,0,0)]
 define [YELLOW] [color(0,0,1,0)]
 define [BLACK] [color(0,0,0,1)]
 define [WHITE] [color(0,0,0,0)]
 define [BLUE] [color(1,1,0,0)]
 define [GREEN] [color(1,0,1,0)]
 define [RED] [color(0,1,1,0)]
 define [FILL(X)] [%fill_index=X]
