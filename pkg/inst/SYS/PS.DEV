 %! SURVO 84C PostScript file[LF]
 /erstr 64 string def[LF]
 /survoerror[S]
 { initmatrix /Times-Roman findfont 12 scalefont setfont[LF]
 newpath 100 700 moveto 500 0 rlineto 0 -100 rlineto[LF]
 -500 0 rlineto closepath gsave 1 setgray fill[LF]
 grestore 1 setlinewidth stroke[LF]
 120 680 moveto (Error in SURVO MM PostScript output\(\256le\):) show[LF]
 120 670 moveto (Errorname: ) show[LF]
 $error /errorname get erstr cvs show[LF]
 120 660 moveto (Command: ) show[LF]
 $error /command get erstr cvs show[LF]
 120 650 moveto (Stack: ) show[LF]
 $error /ostack get { erstr cvs show ( - ) show } forall[LF]
 showpage stop } def[LF]
 errordict /handleerror {survoerror} put[LF]
 /survodict 300 dict def survodict begin[LF]
 /m { 0.28346 mul } def[S]  / 0.1 mm scaling
 /di { 6 mul } def[S]       / inch/12=pica
 /lmar 72 def[LF]
 /ymax 804 def[S]
 /xmax 552 def[S]
 /ypos ymax def[S]
 /lsp 12 def[LF]
 /f_cyan 0 def /f_mage 0 def /f_yell 0 def /f_black 1 def[LF]
 /indsize 0.7 def /sublevel -0.3 def /suplevel 0.4 def[LF]
 /direction 1 def[S]
 /prind true def[S]
 /vst 0 def /hst 0 def[LF]
 /init { lmar ypos moveto /prmode 0 def newpage } def[LF]
 /newline { /ypos ypos lsp sub def lmar ypos moveto } def[LF]
 /newpage { /ypos ymax def lmar ypos moveto } def[LF]
 /sfont { dup /fsize exch def exch findfont exch scalefont setfont } def[LF]
 /sresize { /b exch def /a exch def currentfont[LB]a 0 0 b 0 0[RB][LF]
          makefont[S]setfont[S] } def[LF]
 /landscape { direction 1 eq {land} if } def[LF]
 /land { gsave xmax lsp add 24 add 0 translate 90 rotate[LF]
         xmax /xmax ymax def /ymax exch def[LF]
         /ypos ymax def lmar ypos moveto /direction 2 def } def[LF]
 /portrait { direction 2 eq {port} if } def[LF]
 /port { grestore xmax /xmax ymax def /ymax exch def[LF]
         /ypos ymax def lmar ypos moveto /direction 1 def } def[LF]
 /outlined { true charpath gsave 1 setgray fill grestore stroke } def[LF]
 /fshadow { /sitalic exch def /sbold exch def /snormal exch def } def[LF]
 /fbold { sbold fsize sfont } def[LF]
 /fnormal { snormal fsize sfont /prmode 0 def } def[LF]
 /fitalic { sitalic fsize sfont } def[LF]
 /fsub { /ffsize fsize def /a fsize indsize mul def /b fsize sublevel[LF]
         mul def 0 b rmoveto snormal a sfont } def[LF]
 /fsubn { /b ffsize sublevel neg mul def[LF]
          0 b rmoveto snormal ffsize sfont } def[LF]
 /fsup { /ffsize fsize def /a fsize indsize mul def /b fsize suplevel[LF]
         mul def 0 b rmoveto snormal a sfont } def[LF]
 /fsupn { /b ffsize suplevel neg mul def[LF]
          0 b rmoveto snormal ffsize sfont } def[LF]
 /funder { /prmode 4 def } def[LF]
 /fdot { /prmode 5 def } def[LF]
 /frev { /prmode 7 def } def[LF]
 /chardim { gsave newpath 0 0 moveto[LF]
 (Aj) false charpath flattenpath pathbbox[LF]
 /b exch def pop /a exch def pop[LF]
 b a sub 0.1 mul /c exch def[LF]
 /a a c sub def /b b c add c add def a b[LF]
 grestore } def[LF]
 /blackbox / x0 y0 len color[LF]
 { /bcol exch def /len exch def chardim /y2 exch def /y1 exch def[LF]
 gsave newpath moveto 0 y1 rlineto len 0 rlineto[LF]
 0 y1 neg y2 add rlineto[LF]
 len neg 0 rlineto[LF]
 closepath bcol setgray fill grestore } def[LF]
 /blackshow / len str[LF]
 { /str exch def /len exch def currentpoint len col_2 blackbox } def[LF]
 /grayshow / len str[LF]
 { /str exch def /len exch def currentpoint len gray blackbox } def[LF]
 /uline / x0 y0 len[LF]
 { /len exch def chardim pop /y1 exch def[LF]
 gsave newpath moveto currentpoint 0 y1 rmoveto len 0 rlineto[LF]
 y1 neg .2 mul setlinewidth stroke grestore moveto } def[LF]
 /undershow / len str[LF]
 { /str exch def /len exch def currentpoint len uline } def[LF]
 /show0 { sstr s_show } def[LF]
 /show4 { sstr dup stringwidth pop exch undershow[LF]
    sstr s_show } def[LF]
 /show5 { sstr dup stringwidth pop exch grayshow[LF]
    sstr s_show } def[LF]
 /show7 { sstr dup stringwidth pop exch blackshow[LF]
    gsave col_1 setgray[LF]
    sstr s_show currentpoint[LF]
    grestore moveto } def[LF]
 /showvec [LB] {show0} 0 0 0 {show4} {show5} 0 {show7} [RB] def[LF]
 /prshow
 { /sstr prline pri get def showvec prmode get exec[LF]
 /pri pri 1 add def } def[LF]
 /showp0 { pop prextra 0 8#040 sstr s_widthshow } def[LF]
 /showp4 { sstr undershow[LF]
    prextra 0 8#040 sstr s_widthshow } def[LF]
 /showp5 { sstr grayshow[LF]
    prextra 0 8#040 sstr s_widthshow } def[LF]
 /showp7 { sstr blackshow[LF]
    gsave 1 setgray prextra 0 8#040[LF]
    sstr s_widthshow currentpoint[LF]
    grestore moveto } def[LF]
 /showpvec [LB] {showp0} 0 0 0 {showp4} {showp5} 0 {showp7} [RB] def[LF]
 /prpshow / len[LF]
 { /sstr prline pri get def showpvec prmode get exec } def[LF]
 /prnshow / string[LF]
 { /sstr exch def showvec prmode get exec } def[LF]
 /prex1 / nspace[LF]
 { prlev prsum sub exch div /prextra exch def[LF]
 ( ) stringwidth pop dup /a exch def a prlev div 300 mul mul[LF]
 endtol mul prextra lt { /prextra 0 def } if[LF]
 a -0.3 mul prextra gt { /prextra a -0.3 mul def } if } def[LF]
 /prex2 / nsp[LF]
 { prextra mul prline pri get stringwidth pop add prpshow[LF]
    /pri pri 1 add def } def[LF]
 /prex3[LF]
 { prline pri get stringwidth pop prsum add /prsum exch def[LF]
    /pri pri 1 add def } def[LF]
 /s_show
 { vst 0 eq hst 0 eq and {show} {s_show2} ifelse } def[LF]
 /s_show2
 { dup currentpoint /s_y exch def /s_x exch def show currentpoint[LF]
    /s_y2 exch def /s_x2 exch def s_x vst add s_y hst add moveto[LF]
    show s_x2 s_y2 moveto } def[LF]
 /s_widthshow
 { vst 0 eq hst 0 eq and {widthshow} {s_widthshow2} ifelse } def[LF]
 /s_widthshow2
 { /s_t exch def /s_c exch def /s_1 exch def /s_2 exch def[LF]
    currentpoint /s_y exch def /s_x exch def[LF]
    s_2 s_1 s_c s_t widthshow currentpoint[LF]
    /s_y2 exch def /s_x2 exch def s_x vst add s_y hst add moveto[LF]
    s_2 s_1 s_c s_t widthshow s_x2 s_y2 moveto } def[LF]
 /sborder { /yb exch def /xb exch def[LF]
    currentpoint /aa exch def xb exch def[LF]
    yb lsp 0.75 mul aa add def } def[LF]
 /eborder { prind { ebox } { pop pop } ifelse } def[LF]
 /ebox { /yc exch def /xc exch def[LF]
    currentpoint /bb exch def /aa exch def[LF]
    /cc bb lsp 0.25 mul sub def[LF]
    newpath xc yc moveto aa yc lineto aa cc lineto[LF]
    xc cc lineto closepath stroke aa bb moveto } def[LF]
 /pstore1 { 2 array def } def[LF]
 /pstore2 { currentpoint /b exch def /a exch def[LF]
            dup 0 a put 1 b put } def[LF]
 /pjump1 { dup 1 get /b exch def 0 get b moveto } def[LF]
 /pjump2 { currentpoint /ypos exch def pop } def[LF]
 /p_init { 0 0 moveto } def[LF]
 /line { /y2 exch def /x2 exch def /y1 exch def /x1 exch def[LF]
         newpath x1 m y1 m moveto x2 m y2 m lineto stroke } def[LF]
 /rl { m exch m exch rlineto } def[LF]
