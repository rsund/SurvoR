 define [JUMP(x)] pointx[S]pjump1[S]pjump2[LF]
 define [COUNT_OFF] [%count_off=1]
 define [COUNT_ON]  [%count_off=0]
 control <: [STORE(col)][COUNT_OFF]    / first column
 control >: [JUMP(col)][COUNT_ON]      / last column
 control =: [JUMP(col)]                / intermediate column
 shadow H: halflf[S][%line_extra=0.5]
