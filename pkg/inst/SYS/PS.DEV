 define [MOVE(X,Y)] [%x_move=X][%y_move=Y]
 define [ROT(A)] [%rotation=A]
 define [ROTATE(A)] [%rotate=A]
 define [END_TOL(x,y)] [%norm_tol=x][%spec_tol=y]
