 /halflf { prind[LF]
 { 0 lsp -2 div rmoveto /ypos ypos lsp 2 div sub def } if } def[LF]
 /footnote_bar { currentpoint gsave newpath moveto[LF]
    36 0 rlineto 0.24 setlinewidth stroke grestore } def[LF]
 /dot { /b exch def /a exch def newpath a 1 add m b 1 add m moveto[LF]
   -2 m -2 m rlineto 2 m 0 rmoveto -2 m 2 m rlineto stroke } def[LF]
 /tempshow { /str 10 string def 10 str cvrs show } def[LF]
 /euro { prind {euro2} if } def[LF]
