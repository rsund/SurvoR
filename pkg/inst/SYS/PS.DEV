    currentpoint (C) stringwidth pop /eu exch def[LF]
    eu 1.1 mul neg[LF]
    eu 0.05 mul rmoveto (=) show moveto grestore } def[LF]
 /
 / Encoding accented characters Ñ,î, etc.
 /reencsmalldict 12 dict def[LF]
 /ReEncodeSmall[LF]
 {reencsmalldict begin[LF]
 /newcodesandnames exch def[LF]
 /newfontname exch def[LF]
 /basefontname exch def[LF]
 /basefontdict basefontname findfont def[LF]
 /newfont basefontdict maxlength dict def[LF]
 basefontdict[LF]
 {exch dup /FID ne[LF]
  {dup /Encoding eq[LF]
   {exch dup length array copy[LF]
    newfont 3 1 roll put}[LF]
   {exch newfont 3 1 roll put}[LF]
   ifelse[LF]
  }[LF]
  {pop pop}[LF]
  ifelse[LF]
 } forall[LF]
 newfont /FontName newfontname put[LF]
 newcodesandnames aload pop[LF]
 newcodesandnames length 2 idiv[LF]
  {newfont /Encoding get 3 1 roll put}[LF]
  repeat[LF]
 newfontname newfont definefont pop[LF]
 end[LF]
 } def[LF]
