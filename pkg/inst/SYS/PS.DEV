 define [ROTATION(x)] x[S]rotate[LF]
 define [SCALING(x,y)] [LF]%SCALING:[LF]x[S]y[S]scale[S]newpage[LF]
 define [PSAVE] psave[LF][%pstatus=1]
 define [PRESTORE] prestore[LF][%pstatus=0]
 define [dstrike(x,y)] [%vstrike=x][%hstrike=y]
 define [NOTE_START] [PSAVE][Times(10)][line_spacing(11)]
